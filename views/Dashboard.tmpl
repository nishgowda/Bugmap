{{ define "Dashboard" }}
  {{ template "Header" }}
    {{ template "Menu"  }}
    <center>
    <h2>Dashboard</h2>
        </center>
    <div class="container">
    <table class="table" >
      <thead>
      <tr>
        <th scope="col">Ongoing Projects</th>
        <th scope="col">Ongoing Issues</th>
      </tr>
       </thead>
       <tbody>
    {{ range .  }}
      <tr>
        <td>{{ .NumProjects }} </td> 
        <td> {{ .NumIssues }} </td>
      </tr>
    {{ end }}
       </tbody>
    </table>
    
    </div>

  {{ template "Footer" }}
{{ end }}


